{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "buildCommand": "npm run build:ignore-lint",
  "framework": null,
  "functions": {},
  "rewrites": []
}
